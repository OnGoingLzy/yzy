<view style="line-height:normal;"><view class="shop-container"><view class="left-container"><view data-event-opts="{{[['tap',[['toShop',['$event']]]]]}}" style="display:flex;align-items:center;" bindtap="__e"><image style="width:18px;height:18px;" src="/static/icon/药店.svg"></image><text>{{shop.simplifyShopName}}</text></view><view data-event-opts="{{[['tap',[['toShop',['$event']]]]]}}" style="padding-left:5px;max-height:20px;color:#c01a04;" bindtap="__e"><text>{{shop.distance?'距您'+shop.distance+'km':'\n'}}</text></view><view data-event-opts="{{[['tap',[['toShop',['$event']]]]]}}" style="padding-left:5px;height:20px;" bindtap="__e"><text class="goods-info-text" style="font-size:13px;color:#8b8b8b;">{{''+shop.shopAddress}}</text></view></view><view data-event-opts="{{[['tap',[['toGoodsDetails',['$0','$1'],['goods','shop']]]]]}}" class="right-container" bindtap="__e"><view style="text-align:right;"><text class="goods-price">{{'￥'+shop.price}}</text></view><view style="justify-content:flex-end;display:flex;"><view><text style="color:#8b8b8b;">库存:</text></view><view style="width:40px;"><text style="color:#c01a04;">{{shop.inventory==null?0:shop.inventory+''+goods.sellUnit}}</text></view></view><view class="button-view"><text style="font-size:13px;">查看商品详情</text></view></view></view></view>