<view class="uni-data-checklist" style="{{'margin-top:'+(isTop+'px')+';'}}"><block a:if="{{!isLocal}}"><view class="uni-data-loading"><block a:if="{{!mixinDatacomErrorMessage}}"><uni-load-more vue-id="15f85772-1" status="loading" iconType="snow" iconSize="{{18}}" content-text="{{contentText}}" onVueInit="__l"></uni-load-more></block><block a:else><text>{{mixinDatacomErrorMessage}}</text></block></view></block><block a:else><block a:if="{{multiple}}"><checkbox-group data-event-opts="{{[['change',[['chagne',['$event']]]]]}}" class="{{(('checklist-group')+' '+((mode==='list'||wrap)?'is-list':''))}}" onChange="__e"><label class="{{((((('checklist-box')+' '+('is--'+mode))+' '+(item.selected?'is-checked':''))+' '+(disabled||!!item.disabled?'is-disable':''))+' '+(index!==0&&mode==='list'?'is-list-border':''))}}" style="{{(item.styleBackgroud)}}" a:for="{{dataList}}" a:for-item="item" a:for-index="index" a:key="index"><checkbox class="hidden" hidden="{{true}}" disabled="{{disabled||!!item.disabled}}" value="{{item[map.value]+''}}" checked="{{item.selected}}"></checkbox><block a:if="{{mode!=='tag'&&mode!=='list'||mode==='list'&&icon==='left'}}"><view class="checkbox__inner" style="{{(item.styleIcon)}}"><view class="checkbox__inner-icon"></view></view></block><view class="{{(('checklist-content')+' '+((mode==='list'&&icon==='left')?'list-content':''))}}"><text class="checklist-text" style="{{(item.styleIconText)}}">{{item[map.text]}}</text><block a:if="{{mode==='list'&&icon==='right'}}"><view class="checkobx__list" style="{{(item.styleBackgroud)}}"></view></block></view></label></checkbox-group></block><block a:else><radio-group data-event-opts="{{[['change',[['chagne',['$event']]]]]}}" class="{{((('checklist-group')+' '+((mode==='list')?'is-list':''))+' '+((wrap)?'is-wrap':''))}}" onChange="__e"><label class="{{((((('checklist-box')+' '+('is--'+mode))+' '+(item.selected?'is-checked':''))+' '+(disabled||!!item.disabled?'is-disable':''))+' '+(index!==0&&mode==='list'?'is-list-border':''))}}" style="{{(item.styleBackgroud)}}" a:for="{{dataList}}" a:for-item="item" a:for-index="index" a:key="index"><radio class="hidden" hidden="{{true}}" disabled="{{disabled||item.disabled}}" value="{{item[map.value]+''}}" checked="{{item.selected}}"></radio><block a:if="{{mode!=='tag'&&mode!=='list'||mode==='list'&&icon==='left'}}"><view class="radio__inner" style="{{(item.styleBackgroud)}}"><view class="radio__inner-icon" style="{{(item.styleIcon)}}"></view></view></block><view class="{{(('checklist-content')+' '+((mode==='list'&&icon==='left')?'list-content':''))}}"><text class="checklist-text" style="{{(item.styleIconText)}}">{{item[map.text]}}</text><block a:if="{{mode==='list'&&icon==='right'}}"><view class="checkobx__list" style="{{(item.styleRightIcon)}}"></view></block></view></label></radio-group></block></block></view>