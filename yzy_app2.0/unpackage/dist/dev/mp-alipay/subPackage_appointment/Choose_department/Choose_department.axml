<view><view style="position:absolute;top:0;width:100%;"><view class="search-container"><view class="search-input"><input class="search-text" type="text" placeholder="科室名" data-event-opts="{{[['input',[['__set_model',['','searchContent','$event',[]]]]]]}}" value="{{searchContent}}" onInput="__e"/><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" style="height:100%;width:40px;text-align:center;" onTap="__e"><text class="voice-icon" style="line-height:30px;">×</text></view><view><button data-event-opts="{{[['tap',[['search']]]]}}" style="height:30px;max-width:20vw;border-radius:30px;background-color:#409EFF;color:#fff;font-size:14px;display:flex;justify-content:center;" onTap="__e"><text style="line-height:30px;">搜索</text></button></view></view></view></view><view class="sort-view"><view><scroll-view class="sort-left" scroll-y="true"><view a:for="{{department}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['sortFun',[index,'$0'],[[['department','',index,'id']]]]]]]}}" class="{{(('sort-first')+' '+((item.id==choiceFirst)?'activesort':''))}}" onTap="__e"><text style="text-align:center;">{{item.departmentName}}</text></view></view></scroll-view></view><view><scroll-view class="sort-right" scroll-y="true"><view data-event-opts="{{[['tap',[['toChooseDoctor',['$0'],[[['departmentSubClass','',index1]]]]]]]}}" onTap="__e" a:for="{{departmentSubClass}}" a:for-item="item1" a:for-index="index1" a:key="index1"><view class="sort-first"><text style="text-align:center;">{{item1.subclassName}}</text></view><view class="line" style="height:1rpx;background-color:#ccc;margin:5px 5px 0 5px;"></view></view></scroll-view></view></view></view>