.add-path{
	width: 100%;
	display: flex;
	justify-content: center;
}
.add-path-btn{
	background: linear-gradient(to right, #1296DB, #1296DB);
	color: #fff;
	border-radius: 50rpx;
	padding: 10rpx 190rpx;
    position: fixed;
    bottom: 60px;
}
.add-path-btn-wx{
	background: linear-gradient(to right, #1296DB, #1296DB);
	color: #fff;
	border-radius: 50rpx;
	padding: 10rpx 180rpx;
    position: fixed;
    bottom: 20px;
}
.path-list{
	padding-left: 20rpx;
	border-radius: 30rpx;
}
.path-item{
	padding: 30rpx 20rpx;
