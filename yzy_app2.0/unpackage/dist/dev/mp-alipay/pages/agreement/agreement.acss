.numbered-list .custom-list.data-v-1a454adc {
  counter-reset: item-counter; /* 为每个列表设置独立的计数器 */
}
.numbered-list .custom-list ._li.data-v-1a454adc {
  list-style-type: none; /* 去除默认列表样式 */
  position: relative;
  padding-left: 40rpx; /* 留出空间给序号和点，调整此值 */
  margin-bottom: 10rpx; /* 调整列表项间的间距 */
}
.numbered-list .custom-list ._li.data-v-1a454adc::before {
  content: counter(item-counter) "."; /* 使用计数器的值加上点作为伪元素内容 */
  counter-increment: item-counter; /* 增加计数器值 */
  position: absolute;
  left: 0;
}

