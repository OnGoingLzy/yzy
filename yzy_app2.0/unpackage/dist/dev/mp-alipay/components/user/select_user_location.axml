<view class="example"><scroll-view style="height:600rpx;" scroll-y="true"><view data-event-opts="{{[['tap',[['checkLocation',['$0'],[[['list','',index]]]]]]]}}" class="path-item" onTap="__e" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="item-main"><view class="item-name-popup item-name-name">{{item.name}}</view><view class="item-name-popup item-name-phone">{{item.tel}}</view></view><view class="item-main" style="align-items:flex-start;margin-left:60rpx;"><block a:if="{{item.isDefault==1}}"><view class="active">默认</view></block><view class="item-name-popup">{{item.city+" "+item.details}}</view></view></view></scroll-view><button class="button-goto-maintain" style="margin-top:20rpx;" type="primary" data-event-opts="{{[['tap',[['goToMaintain',['$event']]]]]}}" onTap="__e">去维护地址</button></view>