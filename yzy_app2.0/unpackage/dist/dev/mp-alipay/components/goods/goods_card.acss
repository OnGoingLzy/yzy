.number-badge {
  position: absolute;
  bottom: 0px;
  /* 距离底部的距离 */
  left: 65px;
  /* 距离左侧的距离 */
  color: white;
  /* 字体颜色 */
  padding: 5px 5px;
  /* 内边距 */
  border-radius: 10px 0 10px 0;
  /* 圆角 */
  height: 10px;
  text-align: center;
  width: 10px;
  line-height: 12px;
  background-color: rgba(98, 98, 98, 0.5);
  /* 这将创建一个半透明的红色背景 */
}
.notTheLastOne {
  border-top: 6px solid #f9f9f9;
}
.goods-item {
  box-sizing: border-box;
  display: flex;
  padding: 10px 5px;
}
.goods-item .goods-item-left {
  margin-right: 15px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.goods-item .goods-item-left .goods-pic {
  width: 85px;
  height: 90px;
  display: block;
  border-radius: 10px;
}
.goods-item .goods-item-right {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: space-between;
}
.goods-item .goods-item-right .goods-bq-box {
  display: inline-grid;
  height: 100%;
  padding: rpx 0;
  font-size: 26rpx;
  align-items: center;
}
.goods-item .goods-item-right .goods-bq-box .space {
  color: #ffffff;
}
.goods-item .goods-item-right .goods-bq-box .goods-bq {
  border: 1px solid #ec7063;
  color: #ec7063;
  text-align: center;
  align-items: left;
  padding: 5rpx 5px 5rpx 5px;
  font-size: 24rpx;
  width: auto;
  color: #ffffff;
  border-radius: 5px;
  line-height: 1;
}
.goods-item .goods-item-right .goods-bq-box .goods-bq2 {
  border: 1px solid #68d568;
  text-align: center;
  align-items: left;
  padding: 5rpx 5px 5rpx 5px;
  font-size: 24rpx;
  width: auto;
  color: #ffffff;
  border-radius: 5px;
  line-height: 1;
}
.goods-item .goods-item-right .goods-desc {
  color: #bebebe;
  font-size: 12px;
  overflow: hidden;
  text-overflow: ellipsis;
  /* 超出部分省略号 */
  display: -webkit-box;
  /** 对象作为伸缩盒子模型显示 **/
  -webkit-box-orient: vertical;
  /** 设置或检索伸缩盒对象的子元素的排列方式 **/
  -webkit-line-clamp: 1;
  /** 显示的行数 **/
}
.goods-item .goods-item-right .goods-name {
  font-size: 14px;
}
.goods-item .goods-item-right .goods-info-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.goods-item .goods-item-right .goods-info-box .space {
  color: #ffffff;
}
.goods-item .goods-item-right .goods-info-box .goods-price {
  color: #c01a04;
  font-size: 15px;
  font-family: fantasy;
}
.goods-box {
  min-height: 40px;
  display: flex;
  align-items: center;
  justify-content: left;
}
.goods-box-name {
  line-height: normal;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  word-break: break-all;
  padding-right: 5px;
}
.has-stock-tag {
  border-radius: 4px;
  border: 1px solid #3498db;
  font-size: 10px;
  color: #3498db;
  text-align: center;
  justify-content: center;
  align-items: center;
  display: flex;
  padding: 1px 2px;
  white-space: nowrap;
}
.is-reserve-tag {
  border-radius: 4px;
  border: 1px solid #ec7063;
  font-size: 10px;
  color: #ec7063;
  text-align: center;
  justify-content: center;
  align-items: center;
  display: flex;
  padding: 1px 2px;
  white-space: nowrap;
}

